    <div class="modal fade" id="addProjectModal" tabindex="-1" role="dialog" aria-labelledby="addProjectModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="addProjectModalLabel">Add / Remove User</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form>
                        <div class="row">
                            <div class="col-md-12">
                                <label class="mb-1">Assign To User</label>
                                <input type="text" placeholder="Task Name" class="form-control mb-2" required>
                            </div>

                            <div class="col-md-12 mb-2">
                                <label class="mb-1">Description</label>
                                <textarea type="text" placeholder="Task Name" class="form-control mb-2" required></textarea>
                            </div>

                            <div class="col-md-12  mb-2">
                                <label class="mb-1">Priority</label>
                                <select class="form-control">
                                    <option>Select Option</option>
                                </select>
                            </div>

                            <div class="col-md-6">
                                <label class="mb-1">Start Date</label>
                                <input type="datetime-local" id="birthdaytime" name="birthdaytime" class="form-control mb-2">
                            </div>

                            <div class="col-md-6">
                                <label class="mb-1">End Date</label>
                                <input type="datetime-local" id="birthdaytime" name="birthdaytime" class="form-control mb-2">
                            </div>

                            <div class="col-md-12 mb-2">
                                <div class="d-flex align-items-center">
                                    <label class="mr-2">Assign To: </label>
                                    <div class="form-check mr-2">
                                        <input class="form-check-input" type="radio" name="exampleRadios" id="exampleRadios1" value="option1" checked>
                                        <label class="form-check-label" for="exampleRadios1">
                                      Default radio
                                    </label>
                                    </div>
                                    <div class="form-check mr-2">
                                        <input class="form-check-input" type="radio" name="exampleRadios" id="exampleRadios2" value="option2">
                                        <label class="form-check-label" for="exampleRadios2">
                                      Second default radio
                                    </label>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-12">
                                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                                <button type="button" class="btn btn-primary border-0" style="background:#ed4724 !important;">Save</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
